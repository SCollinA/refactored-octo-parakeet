{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": {
            "options": "tabs"
		},
		"variable-name": {
			"options": [
				"ban-keywords",
				"allow-leading-underscore",
				"require-const-for-all-caps"
			]
		}
    },
    "rulesDirectory": []
}